{
    "name": "joseluisq/gimage",
    "description": "A simple PHP library for easy image handling.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "image",
        "gd",
        "png",
        "jpg",
        "jpeg",
        "resize",
        "crop",
        "file",
        "save",
        "handler"
    ],
    "authors": [{
        "name": "JosÃ© Luis Quintana",
        "homepage": "https://git.io/joseluisq",
        "role": "Owner"
    }],
    "support": {
        "issues": "https://github.com/joseluisq/gimage/issues"
    },
    "require": {
        "php": ">=5.5",
        "guzzlehttp/psr7": "^1.3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "GImage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GImage\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "ext-gd": "To use GD library based on image processing."
    }
}
